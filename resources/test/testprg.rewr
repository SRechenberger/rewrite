true -> true.
false -> false.
(if 0 THEN ELSE) -> ELSE.
(if I THEN ELSE) -> THEN.
(loop 0 VAL ACC) -> ACC.
(loop (s I) VAL ACC) -> (loop I VAL (cons (calc (s I) VAL) ACC)).
(calc N VAL) -> (add N VAL). 
(mult 0 M) -> 0.
(mult (s N) M) -> (add M (mult N M)).
(add 0 M) -> M.
(add (s N) M) -> (add N (s M)).
(exp N 0) -> one.
(exp N (s M)) -> (mult N (exp N M)). 
1 -> (s 0).
2 -> (s 1).
3 -> (s 2).
4 -> (s 3).
5 -> (s 4).
omega -> omega2.
omega2 -> omega.


{nat 0}.
{zero 0}.
{nat N} -> {nat (s N)}.
{nat N} -> {nonzero (s N)}.
{nat N}, {nat M} -> {nat (add N M)}.
{nonzero N} -> {nonzero (add N M)}.
{nonzero M} -> {nonzero (add N M)}.

{P THEN}, {P ELSE} -> {P (if X THEN ELSE)}.
